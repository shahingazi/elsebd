    <!-- Tagline section -->
    <section id="tagline3">
        <div class="container">
            <span class="taglineText3">
                <span class="taglinesSubText3">{elsebd},</span> your true business partner.
            </span>
        </div>
    </section>

    <!-- Team Section -->
    <section id="team" class="bg-light-gray">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <h2 class="section-heading">Our Super Cool Team</h2>
                    <span class="section-underline"></span>
                    <h3 class="section-subheading text-muted">Heroes behind the scenes of your project success.</h3>
