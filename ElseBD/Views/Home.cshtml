@inherits Umbraco.Web.Mvc.UmbracoTemplatePage<ContentModels.Home>
@using ContentModels = Umbraco.Web.PublishedContentModels;
@{
	Layout = "Master.cshtml";
}


 <!-- Header -->
    <header>
        <div class="container">
            <div class="intro-text">
                <div class="intro-lead-in">Hello World;</div>
                <div class="intro-heading">We're here to bring your stories to life!</div>
                <a href="#about" class="page-scroll btn btn-xl">Show me how</a>
            </div>
        </div>
    </header>

    <!-- About Section -->
    <section id="about">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <h2 class="section-heading">How we work?</h2>
                    <span class="section-underline"></span>
                    <h3 class="section-subheading text-muted">The path to your success.</h3>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12">
                    <ul class="timeline">
                        <li>
                            <div class="timeline-image">
                                <img class="img-circle img-responsive" src="img/about/1.jpg" alt="">
                            </div>
                            <div class="timeline-panel">
                                <div class="timeline-heading">
                                    <h4>Step: 1</h4>
                                    <h4 class="subheading">Review User Story</h4>
                                </div>
                                <div class="timeline-body">
                                    <p class="text-muted">Client brings an user story and we take care of the rest! Starting from analyzing project challenges to technical specification, everything is taken into consideration while evaluating the user story.</p>
                                </div>
                            </div>
                        </li>
                        <li class="timeline-inverted">
                            <div class="timeline-image">
                                <img class="img-circle img-responsive" src="img/about/2.jpg" alt="">
                            </div>
                            <div class="timeline-panel">
                                <div class="timeline-heading">
                                    <h4>Step: 2</h4>
                                    <h4 class="subheading">Give Quotation</h4>
                                </div>
                                <div class="timeline-body">
                                    <p class="text-muted">Based on the challenges identified, a report is generated proposing a quotation to implement the user story. Once the client agrees with the quotation, we move forward.</p>
                                </div>
                            </div>
                        </li>
                        <li>
                            <div class="timeline-image">
                                <img class="img-circle img-responsive" src="img/about/3.jpg" alt="">
                            </div>
                            <div class="timeline-panel">
                                <div class="timeline-heading">
                                    <h4>Step: 3</h4>
                                    <h4 class="subheading">Present Technical Specification</h4>
                                </div>
                                <div class="timeline-body">
                                    <p class="text-muted">Next, a technical specification is created which contains high level architecture, project management methodologies and development environment details etc. Client reviews and signs this specification document.</p>
                                </div>
                            </div>
                        </li>
                        <li class="timeline-inverted">
                            <div class="timeline-image">
                                <img class="img-circle img-responsive" src="img/about/4.jpg" alt="">
                            </div>
                            <div class="timeline-panel">
                                <div class="timeline-heading">
                                    <h4>Step: 4</h4>
                                    <h4 class="subheading">Develop and Implement User Story</h4>
                                </div>
                                <div class="timeline-body">
                                    <p class="text-muted">As soon as technical specification document is completed, our developement team starts implementation which is also a continuous integration.</p>
                                </div>
                            </div>
                        </li>
                        <li>
                            <div class="timeline-image">
                                <img class="img-circle img-responsive" src="img/about/5.jpg" alt="">
                            </div>
                            <div class="timeline-panel">
                                <div class="timeline-heading">
                                    <h4>Step: 5</h4>
                                    <h4 class="subheading">Deploy Project</h4>
                                </div>
                                <div class="timeline-body">
                                    <p class="text-muted">When the development and testing phases are completed by the team, the project gets deployed. A detailed documentation of the project is also created for the users</p>
                                </div>
                            </div>
                        </li>
                        <li class="timeline-inverted">
                            <div class="timeline-image last">
                                <h4>And your amazing story
                                    <br>is Live!
                                </h4>
                            </div>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </section>

    <!-- Tagline section -->
    <section id="tagline1">
        <div class="container">
            <div class="row multi-columns-row">
                <div class="col-sm-4 col-md-4 col-lg-4">
                    <i class="fa fa-code icon"></i>
                    <div class="number">
