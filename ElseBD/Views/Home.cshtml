    <!-- Contact Section -->
    <section id="contact">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <h2 class="section-heading">Contact Us</h2>
                    <span class="section-underline"></span>
                    <h3 class="section-subheading contact-text-not-muted">Always at your service.</h3>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12">
                    <form name="sentMessage" id="contactForm" novalidate>
                        <form >
                        <div class="row">
                            <div class="col-md-6">
                                <div class="form-group">
                                    <input type="text" class="form-control" placeholder="Your Name *" id="name" required data-validation-required-message="Please enter your name.">
                                    <p class="help-block text-danger"></p>
                                </div>
                                <div class="col-md-6 contactCol1">
                                    <div class="form-group">
                                        <input type="email" class="form-control" placeholder="Your Email *" id="email" required data-validation-required-message="Please enter your email address.">
                                        <p class="help-block text-danger"></p>
                                    </div>
                                </div>
                                <div class="col-md-6 contactCol2">
                                    <div class="form-group">
                                        <input type="tel" class="form-control" placeholder="Your Phone *" id="phone" required data-validation-required-message="Please enter your phone number.">
                                        <p class="help-block text-danger"></p>
                                    </div>
                                </div>    
                                <div class="form-group">
                                    <textarea class="form-control" placeholder="Your Message *" id="message" required data-validation-required-message="Please enter a message."></textarea>
                                    <p class="help-block text-danger"></p>
                                </div>
                            </div>
                            <div class="col-md-6 contactadd">
                                <i class="fa fa-phone fa-3x contacticon" aria-hidden="true"></i>&nbsp
                                <span>+880 17 15154092</span><br>
                                <i class="fa fa-home fa-3x contacticon" aria-hidden="true"></i>&nbsp
                                <span>
								31, Bangabandhu Avenue, Dhaka-1000, Bangladesh
								</span><br>
                                <i class="fa fa-envelope fa-3x contacticon" aria-hidden="true"></i>&nbsp
                                <span>
								
								<a href="mailto:contact@elsebd.com" style="color:white"> contact@elsebd.com</a></span>
                            </div>
                            <div class="clearfix"></div>
                            <div class="col-lg-12 text-center">
                                <div id="success"></div>
                                <button type="submit" class="btn btn-xl">Send Message</button>
                                <button type="reset" class="btn btn-xl">Reset</button>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </section>
